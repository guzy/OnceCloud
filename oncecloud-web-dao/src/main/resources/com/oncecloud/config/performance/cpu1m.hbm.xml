<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.oncecloud.entity.performance.Cpu1m" table="cpu_1m" lazy="false" >
	    <composite-id>
	    	<key-property name="time" column="t" type="java.lang.Long"></key-property>
            <key-property name="uuid" column="id" type="java.lang.String"></key-property>
            <key-property name="cpuId" column="cpu_id" type="java.lang.Integer"></key-property>  
        </composite-id>
		<property name="usage" type="java.lang.Double">
        	<column name="usage"/>
        </property>
	</class>
</hibernate-mapping>