<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.oncecloud.entity.performance.Pif1m" table="pif_1m" lazy="false" >
	    <composite-id>
	    	<key-property name="time" column="t" type="java.lang.Long"></key-property>
            <key-property name="uuid" column="id" type="java.lang.String"></key-property>
            <key-property name="pifId" column="pif_id" type="java.lang.Integer"></key-property>
        </composite-id>
		<property name="rxd" type="java.lang.Double">
        	<column name="rxd"/>
        </property>
		<property name="txd" type="java.lang.Double">
        	<column name="txd"/>
        </property>
	</class>
</hibernate-mapping>