<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.oncecloud.entity.performance.Memory6h" table="mem_6h" lazy="false" >
	    <composite-id>
	    	<key-property name="time" column="t" type="java.lang.Long"></key-property>
            <key-property name="uuid" column="id" type="java.lang.String"></key-property>
        </composite-id>
		<property name="totalSize" type="java.lang.Double">
        	<column name="total"/>
        </property>
		<property name="freeSize" type="java.lang.Double">
        	<column name="free"/>
        </property>
	</class>
</hibernate-mapping>