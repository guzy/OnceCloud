.graph-wrapper {
	margin: 20px;
	background: #fff;
	padding: 20px 50px 50px;
	overflow: hidden;
	position: relative;
	
	background: #fff;
	border-top: 1px solid #f3f3f3;
	-webkit-box-shadow: 0 6px 6px rgba(0,0,0,0.175);
	-moz-box-shadow: 0 6px 6px rgba(0,0,0,0.175);
	box-shadow: 0 6px 6px rgba(0,0,0,0.175);
}

.graph-title {
	text-transform: uppercase;
	border-bottom: 1px solid #eee;
	margin-bottom: 30px;
	line-height: 3em;
}

.graph-wrapper .actions {
   z-index: 5;
}

.graph-wrapper .actions, .graph-wrapper .components {
   position: relative;
}

.graph-actions .btn:first-child, .graph-actions .btn-forbidden:first-child {
    border-top: 5px solid #1f90c8;
}

.graph-actions .btn-forbidden {
   color: #777;
}

.graph-actions .btn, .graph-actions .btn-forbidden {
	display: block;
	background: #333;
	border-top: 1px solid #3d3d3d;
	border-bottom: 1px solid #2e2e2e;
	color: #fff;
	-webkit-border-radius: 0;
	-moz-border-radius: 0;
	border-radius: 0;
	padding: 15px 12px;
	width: 48px;
	position: relative;
}


.graph-actions .btn .text, .graph-actions .btn-forbidden .text {
	display: none;
	position: absolute;
	top: 5px;
	left: 60px;
	padding: 12px;
	/* height: 22px;
	line-height: 22px; */
	width: 100px;
	background: #333;
	padding: 8px;
	color: #fff;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	border-radius: 4px;
}

.graph-actions .glyphicon{
	font-size :12px;
	vertical-align: top;
}

